    "authors": [
      {
        "name": "Will Schroeder",
        "email": "will.j.schroeder@gmail.com"
      }
    ],
    "links": {
      "repository": "https://example.com/"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true
  }
}
