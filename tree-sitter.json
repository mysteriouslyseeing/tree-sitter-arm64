    {
      "name": "arm64",
      "camelcase": "Arm64",
      "scope": "source.arm64",
      "path": ".",
      "file-types": [".s", ".S"],
      "injection-regex": "^(aarch64|arm64)$"
    }
  ],
  "metadata": {
